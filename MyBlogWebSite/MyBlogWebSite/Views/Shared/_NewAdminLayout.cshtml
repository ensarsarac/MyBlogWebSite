
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("/Views/NewAdmin/HeaderPartial.cshtml") 

<body>
    <div class="container-scroller">
        <!-- partial:partials/_sidebar.html -->
        @await Component.InvokeAsync("AdminSideBar") 

        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_navbar.html -->
            @await Html.PartialAsync("/Views/NewAdmin/Navbar.cshtml") 
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    @RenderBody()
                </div>
                @await Html.PartialAsync("/Views/NewAdmin/FooterPartial.cshtml")

            </div>
        </div>
    </div>
    @await RenderSectionAsync("Scripts",false) 
    @await Html.PartialAsync("/Views/NewAdmin/ScriptPartial.cshtml")


</body>
</html>

